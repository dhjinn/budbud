<%= simple_form_for @transaction, :url => create_path do |f| %>
  <%= f.collection_select(:club_name, Admin.all, :club_name, :club_name) %>
  <%= f.input :date, required: true %>
  <%= f.input :value, required: true %>
  <%= f.input :notes, required: true %>


  <%= f.button :submit %>
<% end %>

<%= link_to "Cancel", users_url, :class => "button medium"%>