<h1 class="title">BUD BUD</h1>
<h3> 
	<% if user_signed_in? %>
	    <div class="welcome"><%= link_to "Go To Dashboard", users_url(current_user.id), :class => 'button_to'  %></div>
	<% elsif admin_signed_in? %>
		<div class="welcome"><%= link_to "Go To Dashboard", admins_url(current_admin.id), :class => 'button_to'  %></div>
	<% else %>
		<div class="welcome"><%= link_to "Admin Sign up", new_admin_registration_path, :class => 'button_to'  %></div> 
	    <div class="welcome"><%= link_to "User Sign up", new_user_registration_path, :class => 'button_to'  %> </div>
	    <div class="welcome"><%= link_to "Admin Login", new_admin_session_path, :class => 'button_to'  %></div>
	    <div class="welcome"><%= link_to "User Login", new_user_session_path, :class => 'button_to'  %></div>
	<% end %>

</h3>

